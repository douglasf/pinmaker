{
  "name": "pinmaker",
  "version": "1.0.0",
  "description": "Generate PDFs with images in circles for pin making",
  "main": "cli/dist/index.js",
  "type": "module",
  "bin": {
    "pinmaker": "./cli/dist/index.js"
  },
  "scripts": {
    "build": "cd cli && tsc",
    "start": "node cli/dist/index.js",
    "dev": "npm run build && node cli/dist/index.js",
    "test": "cd cli && vitest run",
    "test:watch": "cd cli && vitest"
  },
  "keywords": [
    "pdf",
    "pins",
    "maker",
    "badges"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "commander": "^12.1.0",
    "pdfkit": "^0.15.0",
    "sharp": "^0.33.5",
    "terminal-kit": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/pdfkit": "^0.13.5",
    "@types/terminal-kit": "^2.5.7",
    "typescript": "^5.6.3",
    "vitest": "^4.0.8"
  }
}
